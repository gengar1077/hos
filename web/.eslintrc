{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@typescript-eslint",
    "prettier"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "prettier"
  ],
  "rules": {
    "no-console": 0,
    "prettier/prettier": 0,
    "@typescript-eslint/no-unused-vars": 0,
    "@typescript-eslint/unbound-method": 0,
    "prefer-const": 0
  }
}